

{{#each project}}
<div class="rounded-md text-gray-700 text-center bg-gray-400 px-4 py-2 m-2 md:w-3/12">
    <h2 class="py-2 font-bold text-xl">{{project.name}}</h2> {{! project title from db will go here }}
    <iframe width="100%" height="300" scrolling="no" frameborder="no" allow="autoplay" src="{{project.url}}"></iframe> {{! embed link will go into src }}
    <div class="flex justify-around py-4">
        <button class="px-3 py-2 rounded-md text-sm font-medium text-white bg-gray-900 focus:outline-none focus:text-white focus:bg-gray-700 w-2/5">Edit</button>
        <button class="px-3 py-2 rounded-md text-sm font-medium text-white bg-gray-900 focus:outline-none focus:text-white focus:bg-gray-700 w-2/5">View Feedback</button>
    </div>
</div>
{{/each}}